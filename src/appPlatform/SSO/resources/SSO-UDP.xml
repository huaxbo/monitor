<?xml version="1.0" encoding="UTF-8"?>
<!-- 
  各配置元素名称不能重名，否则将覆盖;
  各配置元素名称已经与程序关联，不能修改！
 -->
<configs>

	<!--  ================================= -->
	<!--      以下是UDP通信服务部分的配置数据     -->
	<!--  ================================= -->

	<!--  发送数据UDP SOCKET端口号	-->
	<sendPort>20000</sendPort>
	<!--  接收数据UDP SOCKET端口号	-->
	<receivePort>20001</receivePort>
	<!--  发送数据超时时长(毫秒)	-->
	<sendTimeOut>2000</sendTimeOut>
	<!--  接收数据超时时长(毫秒)	-->
	<receiveTimeOut>2000</receiveTimeOut>
	<!--  接收数据字节长	-->
	<receiveDataLen>1024</receiveDataLen>


	<!-- 线程池最大工作线程数 -->
	<maxThreadNum>10</maxThreadNum>
	<!-- 线程池最小工作线程数 -->
	<minThreadNum>5</minThreadNum>
	<!-- 
	      线程池的工作线程，当其空闲时长为配置值时，
	      线程池将回收线程，以使线程池保持最小线程数  
	      单位为毫秒(1秒=1000毫秒)
	      限制不能小于1000
	 -->
	<freeTimeout>60000</freeTimeout>
	<!-- 
	      线程池的工作线程，当其不间断工作时长超过配置值时，
	      系统将认为线程已经了崩溃，将强制清除  
	      单位为毫秒(1秒=1000毫秒)
	      限制不能小于1000
	 -->
	<busyTimeout>60000</busyTimeout>
	
	
	<!--  ================================= -->
	<!--      以下是SSO服务部分的配置数据         -->
	<!--  ================================= -->
	<!-- 会话时长(分钟) -->
	<sessionTime>120</sessionTime>
	
</configs>
<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="glo" namespace="/glo" extends="struts-default">
		<action name="logoutAction_*" method="{1}" class="logoutAction">
		</action>
		
		<action name="globalAction_*" method="{1}" class="globalAction">
			<result name="blank">/globalJsp/include/blank.jsp</result>
		</action>
	</package>
</struts>



